{
    "Italics": {
        "prefix": [ "ita" ],
        "body": [
            "\\textit{$1}$2"
        ],
        "description": "Italic text"
    },
    "Bold": {
        "prefix": [ "bf" ],
        "body": [
            "\\textbf{$1}$2"
        ],
        "description": "Boldface text"
    },
    "Normal text": {
        "prefix": [ "rm" ],
        "body": [
            "\\textrm{$1}$2"
        ],
        "description": "Removes all text attributes. Makes normal text in math mode."
    },
    "Margin note": {
        "prefix": [ "mnote" ],
        "body": [
            "\\marginnote{$1}$2"
        ],
        "description": "Note to display on the margin line"
    },
    "Enumerate": {
        "prefix": [ "enum" ],
        "body": [
            "\\begin{enumerate}",
            "\t$1",
            "\\end{enumerate}"
        ],
        "description": "Enumerate environment"
    },
    "Item": {
        "prefix": [ "it" ],
        "body": [
            "\\item "
        ],
        "description": "Environment item"
    },
    "Begin": {
        "prefix": [ "begin" ],
        "body": [
            "\\begin{$1}",
            "\t$2",
            "\\end{$1}"
        ],
        "description": "Begins and ends an environment"
    }
}
