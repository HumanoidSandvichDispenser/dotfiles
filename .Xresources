#define base8  #282828
! DarkRed + Red
#define base1  #fb4934
#define base9  #fb4934
! DarkGreen + Green
#define base2  #b8bb26
#define base10 #b8bb26
! DarkYellow + Yellow
#define base3  #fabd2f
#define base11 #fabd2f
! DarkBlue + Blue
#define base4  #83a598
#define base12 #83a598
! DarkMagenta + Magenta
#define base5  #d3869b
#define base13 #d3869b
! DarkCyan + Cyan
#define base6  #8ec07c
#define base14 #8ec07c
! LightGrey + White
#define base7  #ebdbb2
#define base15 #ebdbb2

! hard contrast: *background: #1d2021
*background: base0
! soft contrast: *background: #32302f
*foreground: base15
! Black + DarkGrey
*color0: base0
*color8: base8
*color1: base1
*color9: base9
*color2: base2
*color10: base10
*color3: base3
*color11: base11
*color4: base4
*color12: base12
*color5: base5
*color13: base13
*color6: base6
*color14: base14
*color7: base7
*color15: base15

st.termname:     st-256color

! Set to a non-zero value to disable window decorations (titlebar, etc) and go borderless.
st.borderless:        0

! --- The following options options can be reloaded via USR1 signal. ---
!st.font:              Iosevka Sandvich,Iosevka Nerd Font:pixelsize=16:antialias=true:autohint=true:style=medium;
*font:              Iosevka Sandvich,Iosevka Nerd Font:pixelsize=16:antialias=true:autohint=true:style=Medium
st.font:              Iosevka Sandvich,Iosevka Nerd Font:pixelsize=16:antialias=true:autohint=true:style=Medium
st.fontalt0:          Twitter Color Emoji:pixelsize=16:antialias=true:autohint=true;

! window padding
st.borderpx:          8

! Available cursor values: 2 4 6 7 = █ _ | ☃ ( 1 3 5 are blinking versions)
st.cursorshape:       2

! thickness of underline and bar cursors
st.cursorthickness:   2

! 0: normal blinking, 1: leave cursor border and blink with cursor's background
st.cursorblinkstyle:  0

! 0: cursor blinks with a constant interval; 1: blinking cycle resets on key input
st.cursorblinkontype: 1

st.disablebold:         0
st.disableitalics:         0
st.disableroman:         0

! Amount of lines scrolled
st.scrollrate:  3

! Kerning / character bounding-box height multiplier
st.chscale:           1.0

! Kerning / character bounding-box width multiplier
st.cwscale:           1.0

! blinking timeout for terminal and cursor blinking (0 disables)
st.blinktimeout:      800

! bell volume. Value between -100 and 100. (0 disables)
st.bellvolume:        100

! 1: render most of the lines/blocks characters without using the font for
! perfect alignment between cells (U2500 - U259F except dashes/diagonals).
! Bold affects lines thickness if boxdraw_bold is not 0. Italic is ignored.
! 0: disable (render all U25XX glyphs normally from the font).
st.boxdraw: 1

! (0|1) boxdraw(bold) enable toggle
st.boxdraw_bold: 1

! braille (U28XX):  1: render as adjacent "pixels",  0: use font
st.boxdraw_braille: 0

! set this to a non-zero value to force window depth
st.depth: 0

! opacity==255 means what terminal will be not transparent, 0 - fully transparent
! (float values in range 0 to 1.0 may also be used)
st.opacity:      255

! (0|1) don't pre-multiply color values of the pixels when using transparency
! (useful for the systems where composite manager not running all the time,
! as color pre-multiplication will make the "opaque" colors look darker/lighter
! without the composition)
st.disable_alpha_correction: 0

st.color16:      #cda869
st.color17:      #9b703f
st.color18:      #323537
st.color19:      #464b50
st.color20:      #838184
st.color21:      #c3c3c3
